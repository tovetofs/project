<!DOCTYPE html>
<html class="no-js" lang="sv">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{% if meta.title %}{{ meta.title }} | {% endif %}{{ site_title }}</title>

    {% if meta.description %}
    <meta name="description" content="{{ meta.description|striptags }}" />
    {% endif %}

    {% if current_page %}
    <link rel="canonical" href="{{ current_page.url }}" />
    {% endif %}

    <link rel="icon" href="{{ base_url }}/image/notklav.png?w=100">

    {# Choose theme #}
    {% if config.session.theme == "dark" %}
        <link rel="stylesheet" href="{{ theme_url }}/css/style-dark.min.css" type="text/css" />
        {# <link rel="preload" fetchpriority="high" as="image" href="{{ base_url }}/image/instruments2.jpg?q=50" type="image/webp"> #}
        <link rel="preconnect" href="https://fonts.googleapis.com"> 
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> 
        <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400&family=Special+Elite&display=swap" rel="stylesheet">
    {% else %}
        <link rel="stylesheet" href="{{ theme_url }}/css/style.min.css" type="text/css" />
        {# <link rel="preload" fetchpriority="high" as="image" href="{{ base_url }}/image/trumpet2.jpg?q=50" type="image/webp"> #}
        <link rel="preconnect" href="https://fonts.googleapis.com"> 
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> 
        <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&family=Poiret+One&display=swap" rel="stylesheet">
    {% endif %}
</head>
